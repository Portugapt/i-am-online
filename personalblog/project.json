{
  "name": "personalblog",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./personalblog/src",
  "targets": {
    "build": {
      "executor": "@analogjs/platform:vite",
      "outputs": [
        "{options.outputPath}",
        "{workspaceRoot}/dist/personalblog/.nitro",
        "{workspaceRoot}/dist/personalblog/ssr",
        "{workspaceRoot}/dist/personalblog/analog"
      ],
      "options": {
        "main": "personalblog/src/main.ts",
        "configFile": "personalblog/vite.config.ts",
        "outputPath": "dist/personalblog/client",
        "tsConfig": "personalblog/tsconfig.app.json"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "sourcemap": false,
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@analogjs/platform:vite-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "personalblog:build",
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "personalblog:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "personalblog:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "personalblog:build"
      }
    },
    "test": {
      "executor": "@analogjs/platform:vitest",
      "outputs": ["{projectRoot}/coverage"]
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "personalblog/.storybook",
        "browserTarget": "personalblog:build-storybook",
        "compodoc": false,
        "styles": ["personalblog/src/styles.css"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/personalblog",
        "configDir": "personalblog/.storybook",
        "browserTarget": "personalblog:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c personalblog/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "personalblog:build-storybook",
        "staticFilePath": "dist/storybook/personalblog"
      },
      "configurations": {
        "ci": {
          "buildTarget": "personalblog:build-storybook:ci"
        }
      }
    },
    "explore": {
      "executor": "source-map-explorer dist/personalblog/client/**/*.js --only-mapped --no-border-checks"
    }
  },
  "tags": []
}
